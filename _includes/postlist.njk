<ul class='blogs-list'>
    {% for post in postlist %}
    <li>
        <h3>
            {{post.data.title}}
        </h3>

        <em>{{post.data.author}}</em>
       
        <p>
            {{ post.data.desc }}... <a href='{{ post.url | url }}' title='{{post.data.title}}'>read more</a>
        </p>
    </li>
    {% endfor %}
</ul>